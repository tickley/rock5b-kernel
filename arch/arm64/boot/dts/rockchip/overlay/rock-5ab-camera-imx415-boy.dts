// ROCK 5A/5B Camera IMX415
/dts-v1/;
/plugin/;

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/pinctrl/rockchip.h>

/ {
	fragment@0 {
		target-path = "/";

		__overlay__ {
			cam_ircut0: cam_ircut {
				status = "okay";
				compatible = "rockchip,ircut";
				ircut-open-gpios = <&gpio1 RK_PB6 GPIO_ACTIVE_HIGH>;
				ircut-close-gpios  = <&gpio1 RK_PA7 GPIO_ACTIVE_HIGH>;
				rockchip,camera-module-index = <0>;
				rockchip,camera-module-facing = "back";
			};
			fan: pwm-fan {
				status = "okay";
				compatible = "pwm-fan";
				#cooling-cells = <2>;
				pwms = <&pwm1 0 10000 0>;
				cooling-levels = <0 128 130 132 135 150>;
				rockchip,temp-trips = <
					50000	1
					55000	2
					60000	3
					65000	4
					70000	5
				>;
			};
		};
	};

	fragment@1 {
		target = <&i2c3>;

		__overlay__ {
			status = "okay";
		};
	};

	fragment@2 {
		target = <&imx415>;

		__overlay__ {
			status = "okay";
			pwdn-gpios = <&gpio4 RK_PA0 GPIO_ACTIVE_HIGH>;
			reset-gpios = <&gpio1 RK_PB0 GPIO_ACTIVE_LOW>;
			lens-focus = <&cam_ircut0>;
		};
	};

	fragment@3 {
		target = <&csi2_dphy0_hw>;

		__overlay__ {
			status = "okay";
		};
	};

	fragment@4 {
		target = <&csi2_dphy0>;

		__overlay__ {
			status = "okay";
		};
	};

	fragment@5 {
		target = <&mipi2_csi2>;

		__overlay__ {
			status = "okay";
		};
	};

	fragment@6 {
		target = <&rkcif>;

		__overlay__ {
			status = "okay";
		};
	};

	fragment@7 {
		target = <&rkcif_mipi_lvds2>;

		__overlay__ {
			status = "okay";
		};
	};

	fragment@8 {
		target = <&rkcif_mipi_lvds2_sditf>;

		__overlay__ {
			status = "okay";
		};
	};

	fragment@9 {
		target = <&rkcif_mmu>;

		__overlay__ {
			status = "okay";
		};
	};

	fragment@10 {
		target = <&isp0_mmu>;

		__overlay__ {
			status = "okay";
		};
	};

	fragment@11 {
		target = <&rkisp0>;

		__overlay__ {
			status = "okay";
		};
	};

	fragment@12 {
		target = <&rkisp0_vir0>;

		__overlay__ {
			status = "okay";
		};
	};
};
